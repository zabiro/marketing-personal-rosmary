<p id="notice"><%= notice %></p>

<h1>Pedidos</h1>

<table>
  <thead>
    <tr>
      <th>Nfactura</th>
      <th>Nombre</th>
      <th>Campana</th>
      <th>Zona</th>
      <th>Nidentificacion</th>
      <th>Telefono</th>
      <th>Tipodepedido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pedidos.each do |pedido| %>
      <tr>
        <td><%= pedido.nfactura %></td>
        <td><%= pedido.nombre %></td>
        <td><%= pedido.campana %></td>
        <td><%= pedido.zona %></td>
        <td><%= pedido.nidentificacion %></td>
        <td><%= pedido.telefono %></td>
        <td><%= pedido.tipodepedido %></td>
        <td><%= link_to 'Show', pedido %></td>
        <td><%= link_to 'Edit', edit_pedido_path(pedido) %></td>
        <td><%= link_to 'Destroy', pedido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pedido', new_pedido_path %>
